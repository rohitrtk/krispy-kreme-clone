---
interface Props {
  title: string;
}

const { title } = Astro.props;
---
<head>
  <meta charset="utf-8" />
  <link rel="icon" href="/favicon.ico" sizes="any" />
  <meta name="viewport" content="width=device-width" />
  <meta name="generator" content={Astro.generator} />
  <title>{title} - Kripsy Kreme</title>
</head>